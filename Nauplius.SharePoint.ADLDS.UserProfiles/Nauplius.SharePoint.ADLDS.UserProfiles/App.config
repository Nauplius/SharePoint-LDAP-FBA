<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="partitionsSection" type="Nauplius.SharePoint.ADLDS.UserProfiles.PartitionsSection, Nauplius.SharePoint.ADLDS.Configuration"/>
  </configSections>
  
  <appSettings>
    <!-- Claims Identifier -->
    <add key="ClaimsIdentifier" value="i:0#.f"/>
    
    <!-- Time in minutes application runs -->
    <add key="ImportTimer" value="30"/>
    
    <!-- LDAP filter -->
    <add key="LDAPObjectFilter" value="(&amp;(objectClass=user))"/>
    
    <!-- Attribute mapping key -->
    <add key="PreferredName" value="displayName"/>
    <add key="WorkPhone" value="telephoneNumber"/>
    <add key="FirstName" value="givenName"/>
    <add key="LastName" value="sn"/>
    <add key="WorkEmail" value="mail"/>
    <add key="Office" value="physicalDeliveryOfficeName"/>
    <add key="Title" value="title"/>
    <add key="Department" value="department"/>
    <add key="WebSite" value="wWWHomePage"/>
  </appSettings>
  
  <partitionsSection xmlns="Nauplius.SharePoint.ADLDS.UserProfiles">
    <partitions>
      <partition server="adlds01" port="636" dn="CN=SharePoint,DC=fabrikam,DC=local" useSSL="true" webApplication="http://fabrikam" logonAttribute="mail" />
    </partitions>
  </partitionsSection>
</configuration>